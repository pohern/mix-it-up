<%- include('../partials/header') %>

<h3 class="cFont1">Users</h3>

<% users.forEach(u => { %>
  <div class="wide">
  <a class='black' href="/users/<%= u._id %>">
    <div class="card color1">
      <div class="card-body">
        <img height="50" id="avatarPhoto" src="<%= u.avatar ? u.avatar : 'http://theoldreader.com/kittens/300/300/' %>" alt=""> 
        <%= u.name %></a> - Joined on <%= u.createdAt.toLocaleString().split(',')[0] %>
      </div>
    </div><br>
  </div>
<% }) %>

<%- include('../partials/footer') %>